<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
     <appSettings>
        <!-- The directory for locating the ScenarioReasoner CGI binaries (.exe) -->
        <!-- An absolute path or a path relative to the executable directory -->
        <add key="cgi_directory" value="../../../cgi" />
        <!-- The directory for locating the scenario XML's (.xml) -->
        <!-- An absolute path or a path relative to the cgi_directory -->
        <add key="xml_directory" value="../../Scenarios" />
    </appSettings>
</configuration>